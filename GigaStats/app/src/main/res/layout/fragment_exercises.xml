<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FRAMELAYOUT_EXERCISES"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingHorizontal="4dip"
    android:paddingTop="2dip"
    tools:context=".fragments.FragmentExercises">

    <ExpandableListView
        android:id="@+id/expandableListView"
        style="@style/backgroundScreen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@color/itemDivider"
        android:dividerHeight="10sp"
        android:groupIndicator="@null"
        android:scrollbars="vertical"
        tools:listitem="@layout/list_group_exercise" />

</LinearLayout>
